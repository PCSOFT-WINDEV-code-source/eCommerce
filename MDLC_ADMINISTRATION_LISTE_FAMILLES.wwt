     internal_properties : CAAAAAgAAACcM0Hdgrovghb2Ky005ItkJ/qUVMHIFWNAFqMVdX6ENoig/GL6/EBH7EWCrfI9Rzmr7Hhs4tDD1nI27CbmtELe/SR90foEjrqTIHq14eKXfbyu/8KdURRHppkv1DdVp9L6bT/SHNycaywtjs7xZu2ID8G8qH03FtG7fvgTMI9V7NnneuPPHUzNxJ+keZA/W1Tvgb4ShqtOUmHyTLADV3m1lo4R5g==
     procedure_id : 2214569237886141612
     type_code : 14
     code : |1-
      // Résumé : Remplit l'arbre des Familles
      // Syntaxe :
      //RempliArbreFamilles ( [<sNoeud> [, <nIdentifiantParent>]])
      //
      // Cette procédure permet de remplir l'arbre des Familles.
      // ATTENTION : ELLE EST RECURSIVE
      // 
      // Paramètres :
      //	sNoeud : Le noeud parent dans lequel il faut ajouter les éléments trouvés
      //	nIdentifiantParent (valeur par défaut=0) : L'identifiant du parent dont il faut rechercher les fils
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE RempliArbreFamilles(sNoeud = CST_NOM_FILTRE_PRODUIT_DEFAUT, nIdentifiantParent = 0)
      
      // Variables locales
      tabListeFamilles 	est un tableau de STCOL_Famille
      stFamille			est un STCOL_Famille
      
      // Recherche des familles
      tabListeFamilles = COL_Famille.ListeFamilles(nIdentifiantParent)
      
      // Parcours des familles
      Pour tout element stFamille de tabListeFamilles
      	// On ajoute la famille dans l'arbre
      	// avec le noeud du parent + TAB + libellé de la famille
      	ArbreAjoute(ARBRE_Familles,sNoeud + TAB + stFamille.Libellé,aDéfaut,aDéfaut,stFamille.IDFamille,aDernier)
      		
      	// On recherche les fils de la famille en cours de traitement
      	RempliArbreFamilles(sNoeud + TAB + stFamille.Libellé,stFamille.IDFamille)
      	
      FIN
     type : 458752
   -
     name : RempliTreeview
     procedure_id : 2214569237886207148
     type_code : 14
     code : |1-
      // Résumé : Remplit l'arbre des Familles
      // Syntaxe :
      // RempliTreeview ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE RempliTreeview()
      
       // Suppression de tout ce qu'il y a dans le TV
      ArbreSupprimeTout(ARBRE_Familles)
      
      // On ajoute la racine de la Famille
      ArbreAjoute(ARBRE_Familles,CST_NOM_FILTRE_PRODUIT_DEFAUT,aDéfaut,aDéfaut,-1,aPremier)
      
      // Remplissage de l'arbre des Familles
      RempliArbreFamilles()
      
      // Déroule tout l'arbre
      ArbreDérouleTout(ARBRE_Familles)
      
      // On recharge toutes les données pour être en phase avec l'arbre
      ChargeFamilles(Vrai,0)
