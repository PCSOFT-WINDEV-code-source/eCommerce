  procedures :
   -
     name : Afficher
     procedure_id : 2206796558454283000
     type_code : 14
     code : |1+
      // Résumé : Affiche une promotion en bandeau
      // Syntaxe :
      //Afficher (<UnePromo> est STCOL_Promotion [, <bAchatVisible>])
      //
      // Paramètres :
      //	UnePromo (STCOL_Promotion) : La promotion à afficher
      //	bAchatVisible (valeur par défaut=0) : Vrai pour afficher les liens d'achats, Faux sinon
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE Afficher(UnePromo est un STCOL_PROMOTION,bAchatVisible = Faux)
      
      
      SI UnePromo:QuantitéLimitée = Vrai ALORS
      	LIB_OffreLimitée = ChaîneConstruit("Offre limitée à %1 produits",UnePromo:Quantité)
      SINON	
      	LIB_OffreLimitée= ""
      FIN
      
      // Est ce que l'on doit afficher le lien acheter ?
      SI bAchatVisible _ET_ UnePromo:IDProduit > 0 ALORS
      	SAI_ID = UnePromo:IDProduit	
      SINON
      	LIEN_CliquezIci..Visible = Faux
      FIN
      
      // Affichage de l'image
      MAP_BANDEAU_PROMOTION = LirePhotoPromotion(UnePromo.IDPromotion,ROLE_PROMOTION_PAYSAGE_GRAND)
     type : 458752
   -
     name : AfficherSelonID
     procedure_id : 2217198874660162133
     type_code : 14
     code : |1+
      // Résumé : Affiche une promotion en bandeau
      // Syntaxe :
      //AfficherSelonID (<nIDPromotion> est entier [, <bAchatVisible>])
      //
      // Paramètres :
      //	nIDPromotion (entier) : La promotion à afficher
      //	bAchatVisible (valeur par défaut=0) : Vrai pour afficher les liens d'achats, Faux sinon
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE AfficherSelonID(nIDPromotion est un entier,bAchatVisible = Faux)
      
      // Variables locales
      stPromotion est un STCOL_Promotion
      
      // Chargement de la promotion
      stPromotion = LirePromotion(nIDPromotion)
      SI stPromotion.IDPromotion = -1 alors
      	// On masque les champs
      	LIEN_CliquezIci..Visible = Faux
      	MAP_BANDEAU_PROMOTION..Visible = Faux
      FIN
      
      SI stPromotion:QuantitéLimitée = Vrai ALORS
      	LIB_OffreLimitée = ChaîneConstruit("Offre limitée à %1 produits",stPromotion:Quantité)
      SINON	
      	LIB_OffreLimitée= ""
      FIN
      
      // Mémorise l'identifiant produit
      SAI_ID = stPromotion:IDProduit	
      
      // Est ce que l'on doit afficher le lien acheter ?
      SI bAchatVisible _ET_ stPromotion:IDProduit > 0 ALORS
      	SAI_ID = stPromotion:IDProduit	
      SINON
      	LIEN_CliquezIci..Visible = Faux
      FIN
      
      // Affichage de l'image
      MAP_BANDEAU_PROMOTION = LirePhotoPromotion(nIDPromotion,ROLE_PROMOTION_PAYSAGE_GRAND)
      
      // S'il n'y a pas de promotion, on masque le champ
      SI MAP_BANDEAU_PROMOTION = "" alors 
      	MAP_BANDEAU_PROMOTION..Visible = Faux
      FIN
     type : 458752
