     internal_properties : CAAAAAgAAAANK99aT578pqrY5vHKk6We4QlF1n4phI60i5kiJ2ajIXqY8ANw+K+Ev0qF5XyF7/X5nkR47wx/A9HQAdXUtV09qS9IbkPPF7nTWSaUTtcQidvXNTL45w==
     code : |1-
      _AjoutHistoriqueSite(CN_HISTORIQUE_TYPEOPE_APPEL_WS,"Appel de la page REST 'admin_rest'")
      
      LOCAL 
      	nIDProduit		est un entier
      	nIDPromotion	est un entier
      	nIDFamille		est un entier
      
      // Est-ce que l'on a un numéro d'opération ?
      SELON PageParamètre(CN_ADMIN_REST_PARAM_IDOPERATION)
      	CAS ~=CN_ADMIN_REST_OPE_GENERATION_PRODUIT
      		NotifierInformation("Génération d'une image de produit : "+PageParamètre(CN_ADMIN_REST_PARAM_IDPRODUIT))
      		
      		// Demande de génération d'une photo de produit
      		nIDProduit = Val(PageParamètre(CN_ADMIN_REST_PARAM_IDPRODUIT))
      		
      		// Vérification du produit
      		SI nIDProduit <=0 alors
      			AjouteErreur("Produit non reconnu",PageParamètre(CN_ADMIN_REST_PARAM_IDPRODUIT))
      			ChaîneAffiche("ERR")				
      		FIN
      		
      		// On lance la génération
      		SI GenerePhotosProduit(nIDProduit) = Faux alors
      			ChaîneAffiche("ERR")
      		FIN
      
      	CAS ~= CN_ADMIN_REST_OPE_GENERATION_FAMILLE
      		NotifierInformation("Génération d'une image de famille de produit : "+PageParamètre(CN_ADMIN_REST_PARAM_IDFAMILLE))
      		
      		// Demande de génération d'une photo de famille
      		nIDFamille = Val(PageParamètre(CN_ADMIN_REST_PARAM_IDFAMILLE))
      		
      		// Vérification de la famille
      		SI nIDFamille <=0 ALORS
      			AjouteErreur("Produit non reconnu",PageParamètre(CN_ADMIN_REST_PARAM_IDFAMILLE))
      			ChaîneAffiche("ERR")				
      		FIN
      		
      		// On lance la génération		
      		SI GenerePhotoFamille(nIDFamille) = Faux ALORS
      			ChaîneAffiche("ERR")
      		FIN	
      	
      	CAS ~= CN_ADMIN_REST_OPE_GENERATION_PROMOTION
      		NotifierInformation("Génération d'une image de promotion : "+PageParamètre(CN_ADMIN_REST_PARAM_IDPROMOTION))
      		
      		// Demande de génération d'une photo de promotion
      		nIDPromotion = Val(PageParamètre(CN_ADMIN_REST_PARAM_IDPROMOTION))
      		
      		// Vérification de la promotion
      		SI nIDPromotion <=0 ALORS
      			AjouteErreur("Promotion non reconnu",PageParamètre(CN_ADMIN_REST_PARAM_IDPROMOTION))
      			ChaîneAffiche("ERR")				
      		FIN
      		
      		// On lance la génération
      		SI GenerePhotosPromotions(nIDPromotion) = Faux ALORS
      			ChaîneAffiche("ERR")
      		FIN	
      	
      	AUTRE CAS
      		AjouteErreur("Opération non reconnue",PageParamètre(CN_ADMIN_REST_PARAM_IDOPERATION))
      		ChaîneAffiche("ERR")	
      FIN
      
      // Par défaut, tout se déroule correctement
      ChaineAffiche("")
      
      CAS EXCEPTION:
      	AjouteErreur("Exception non gérée",ExceptionInfo())
      	ChaineAffiche("ERR")
